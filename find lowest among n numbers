#include<stdio.h>
main()
{
	int a,b,c,d,e;
	printf("enter any 5 numbers");
	scanf("%d %d %d %d %d",&a,&b,&c,&d,&e);
	if((a<b)&&(a<c)&&(a<d)&&(a<e))
	{
		printf("lowest is:%d",a);
	}
	else 
	if((a>b)&&(b<c)&&(b<d)&&(b<e))
	{
		printf("lowest is:%d",b);
	}
	else
	if((a>c)&&(b>c)&&(c<d)&&(c<e))
	{
		printf("lowest is:%d",c);
	}
	else
	if((d<a)&&(d<b)&&(d<c)&&(d<e))
	{
		printf("lowest is:%d",d);
	}
	else
	if((e<a)&&(e<b)&&(e<c)&&(e<d))
	{
		printf("lowestÂ is:%d",e);
	}
}
